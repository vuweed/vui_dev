sendln "reset"
mpause 700

:OTP_protection
sendln 'lwr 40120000 38000000'
wait '[BOOT]' '[MROM]'

:RETRY
sendln 'lrd 40103ffc'
wait '[0x40103FFC] : '
recvln

strcopy inputstr 25 1 substr
strcompare substr '1'

if result=0 then 
	goto END
else  
	messagebox 'secure regien lock' "confirm"
	sendln 'lwr 40120000 34000000'
	wait '[BOOT]' '[MROM]'
	sendln 'lwr 40103ffc 04000000'
	wait '[BOOT]' '[MROM]'
	sendln 'lwr 40120000 38000000'
	wait '[BOOT]' '[MROM]'
	goto RETRY
goto END
endif
:END 

:download_DA16xxx_ueboot
    getdir srcfile
    strconcat srcfile '\\'
    strconcat srcfile "DA16200_FBOOT-GEN01-01-834dea5b8_W25Q32JW.img"
    send "loady boot" #13#10
    waitln "Load Y-Modem"
    mpause 400
    ymodemsend srcfile
    mpause 4000
    send  #13#10
    waitln "[MROM]"
	
:download_DA16xxx_RTOS
    getdir srcfile
    strconcat srcfile '\\'
    strconcat srcfile "DA16200_FRTOS-GEN01-01-07bc5ab6e-002784.img"
    send "loady 23000" #13#10
    waitln "Load Y-Modem"
    mpause 400
    ymodemsend srcfile
    mpause 4000
    send  #13#10
    waitln "[MROM]"

:changing_boot_index
    send "sflash erase 22000 100" #13#10
	
sendln "boot"